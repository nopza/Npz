@model UserViewModel
@{
    ViewData["Title"] = "Register";
}


<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">User List</h2>
        </div>
    </div>
    <br /><br />
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Username</th>
                <th scope="col">Password</th>
                <th scope="col">Name</th>
                <th scope="col">Age</th>
            </tr>
        </thead>
        @for (int i = 0; i < Model.UserModelList.Count; i++)
        {

            <tbody>
                <tr>
                    <td>@(i+1)</td>
                    <td>@Model.UserModelList[i].Username</td>
                    <td>@Model.UserModelList[i].Password</td>
                    <td>@Model.UserModelList[i].Name</td>
                    <td>@Model.UserModelList[i].Age</td>
                </tr>
            </tbody>

        }
    </table>

</div>

